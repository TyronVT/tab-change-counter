{
  "manifest_version": 3,
  "name": "Tab Switch Counter",
  "version": "1.0",
  "description": "Number of times you switched tabs",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "action": {
    "default_popup": "popup.html"
  }
  
}
